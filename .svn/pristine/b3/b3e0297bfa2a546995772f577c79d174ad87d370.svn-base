.head {
  background-color: #E4393C;
}
.body .bcj-head-cat {
  height: 38px;
  background-color: #fff;
  border-bottom: 1px solid #e6e6e6;
  box-sizing: border-box;
  position: relative;
}
.body .bcj-head-cat .tabs {
  float: left;
  background-color: #FFFFFF;
  border-bottom: none;
  width: 100%;
  overflow: hidden;
  height: 38px;
}
.body .bcj-head-cat .tabs .ui-navigator-list {
  display: table;
  table-layout: fixed;
  white-space: nowrap;
  width: auto;
}
.body .bcj-head-cat .tabs .ui-navigator-list li {
  height: 38px;
  font-size: 14px;
  line-height: 36px;
  display: inline-block;
  font-weight: bold;
  text-align: center;
  white-space: nowrap;
  list-style: none;
}
.body .bcj-head-cat .tabs .ui-navigator-list li a {
  color: #333333;
  display: inline-block;
  font-weight: normal;
  text-decoration: none;
  text-align: center;
  overflow: hidden;
  float: left;
  padding: 0;
  margin: 0 12px;
  width: auto;
}
.body .bcj-head-cat .tabs .ui-navigator-list .ui-state-active a {
  color: #e4393c;
  border-bottom: 2px solid #e4393c;
}
.body .bcj-head-cat .searchbtn {
  position: absolute;
  right: 0;
  width: 38px;
  height: 38px;
  border-left: 1px solid #e6e6e6;
  background-color: #fbfbfb;
  box-sizing: border-box;
  border-bottom: 1px solid #e6e6e6;
}
