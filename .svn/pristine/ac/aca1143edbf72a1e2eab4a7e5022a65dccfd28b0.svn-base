.head {
  background-color: #E4393C;
  min-width: 320px;
}
.body {
  min-width: 320px;
}
.body .bcj-head-cat {
  height: 38px;
  background-color: #fff;
  border-bottom: 1px solid #e6e6e6;
  box-sizing: border-box;
  position: relative;
}
.body .bcj-head-cat .tabs {
  float: left;
  background-color: #FFFFFF;
  border-bottom: 1px solid #e6e6e6;
  width: 100%;
  overflow: hidden;
  height: 38px;
}
.body .bcj-head-cat .tabs .ui-navigator-list {
  display: table;
  table-layout: fixed;
  white-space: nowrap;
  width: auto;
  box-sizing: border-box;
  height: 38px;
}
.body .bcj-head-cat .tabs .ui-navigator-list li {
  height: 38px;
  font-size: 14px;
  line-height: 36px;
  display: inline-block;
  text-align: center;
  white-space: nowrap;
  list-style: none;
  color: #333333;
  font-weight: normal;
  text-decoration: none;
  overflow: hidden;
  padding: 0 12px;
  width: auto;
  box-sizing: border-box;
}
.body .bcj-head-cat .tabs .ui-navigator-list .ui-state-active {
  color: #e4393c;
  border-bottom: 2px solid #e4393c;
}
.body .bcj-head-cat .searchbtn {
  position: absolute;
  right: 0;
  width: 38px;
  height: 38px;
  border-left: 1px solid #e6e6e6;
  background-color: #fbfbfb;
  box-sizing: border-box;
  border-bottom: 1px solid #e6e6e6;
}
.body .adver {
  text-align: center;
  margin-top: 10px;
  margin-bottom: 10px;
  width: 100%;
  background-color: #fbfbfb;
}
.body .adver img {
  width: 100%;
}
.body .container .bcj-list .ui-border-tb {
  background: #fff;
  position: relative;
  height: 150px;
  margin-top: 5px;
}
.body .container .bcj-list .ui-border-tb .t {
  position: absolute;
  left: 140px;
  top: 10px;
  font-size: 14px;
}
.body .container .bcj-list .ui-border-tb .p {
  position: absolute;
  left: 150px;
  top: 40px;
  font-size: 24px;
  color: #fc353a;
  margin-top: 4px;
  width: 100%;
  height: auto;
  overflow: visible;
}
.body .container .bcj-list .ui-border-tb .p del {
  text-decoration: line-through;
  font-size: 13px;
  color: #a8a8a8;
  margin-left: 12px;
  vertical-align: middle;
}
.body .container .bcj-list .ui-border-tb .pic {
  width: 130px;
  height: 130px;
  position: absolute;
  left: 5px;
  top: 10px;
}
.body .container .bcj-list .ui-border-tb .pic img {
  width: 130px;
  height: 130px;
}
.body .container .bcj-list .ui-border-tb .y {
  position: absolute;
  left: 150px;
  top: 120px;
}
.body .container .bcj-list .ui-border-tb .y a {
  padding: 5px;
  border: 1px dotted #e4393c;
  color: #e4393c;
}
.body .container .bcj-list .ui-border-tb .d {
  position: absolute;
  right: 10px;
  top: 120px;
}
.body .container .bcj-list .ui-border-tb .d a {
  padding: 5px 10px;
  background-color: #ff5e5d;
  color: #fff;
  border-radius: 5px;
}
.body .container .bcj-list .ui-border-tb .bar {
  position: absolute;
  left: 150px;
  top: 90px;
}
.body .container .bcj-list .ui-border-tb .bar a {
  position: absolute;
  width: 115px;
  height: 20px;
}
.body .container .bcj-list .ui-border-tb .bar i {
  display: inline-block;
  width: 50px;
  border: 1px solid #fb1e23;
  background-color: #ffb7b7;
  border-radius: 20px;
  position: relative;
  height: 15px;
  overflow: hidden;
}
.body .container .bcj-list .ui-border-tb .bar i em {
  display: inline-block;
  background-color: #fb1e23;
  height: 100%;
  border-radius: 20px;
}
.body .container .bcj-list .ui-border-tb .bar i span {
  position: absolute;
  left: 30%;
  font-weight: bold;
  color: #fff;
}
.body .container .bcj-list .ui-border-tb .bar b {
  margin-left: 10px;
  vertical-align: top;
  color: #fc282e;
}
.foot {
  margin-top: 20px;
  background-color: #efefef;
  border-top: 1px solid #c4c4c4;
  border-bottom: 1px solid #c4c4c4;
  box-sizing: border-box;
}
.foot a {
  display: inline-block;
  width: 32%;
  height: 30px;
  line-height: 30px;
  text-align: center;
  box-sizing: border-box;
}
.foot a img {
  width: 15px;
  height: 15px;
  vertical-align: text-bottom;
}
.foot a:first-of-type {
  border-right: 1px solid #c4c4c4;
}
.foot a:last-of-type {
  border-left: 1px solid #c4c4c4;
}
.footer {
  background: #efefef;
  text-align: center;
  font-size: 12px;
  padding: 15px 0;
  color: #666;
}
.footer p a {
  font-size: 12px;
}
.footer p #AppDown {
  color: red;
}
.backTop {
  position: fixed;
  right: 5px;
  bottom: 20px;
}
.backTop a {
  display: inline-block;
  width: 40px;
  height: 40px;
  border-radius: 50%;
  background-color: rgba(0, 0, 0, 0.8);
  color: #fff;
  line-height: 40px;
  text-align: center;
  font-size: 36px;
}
.App {
  position: fixed;
  right: -64px;
  bottom: 80px;
}
.App a img {
  width: 50%;
  height: 50%;
}
