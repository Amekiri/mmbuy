.loading {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 999;
  background-color: rgba(68, 68, 68, 0.5);
  display: none;
  .loading-img {
    position: absolute;
    left: 50%;
    top: 50%;
    z-index: 1000;
    height: 50px;
    width: 50px;
    transform:translate(-50%,-50%);
  }
}