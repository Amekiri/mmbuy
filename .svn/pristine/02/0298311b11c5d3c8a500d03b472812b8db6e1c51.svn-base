.layout {
  background-color: #f2f2f2;
}
.layout .fixed {
  position: fixed;
  top: 0;
  left: 0;
}
.layout .color {
  background-color: transparent;
}
.layout > .head {
  width: 100%;
  height: 100%;
  background-color: #ff841d;
  height: 50px;
  position: relative;
  padding-bottom: 10px;
}
.layout > .head > .logo {
  width: 100%;
  height: 100%;
}
.layout > .head > .logo > a {
  width: 100%;
  display: inline-block;
}
.layout > .head > .logo > a > img {
  width: 185px;
  height: 25px;
  line-height: 25px;
  position: absolute;
  top: -8px;
  left: 17px;
  display: inline-block;
}
.layout > .head > .appdown {
  width: 20%;
  height: 100%;
}
.layout > .head > .appdown > a {
  width: 100%;
  display: inline-block;
}
.layout > .head > .appdown > a > img {
  width: 34px;
  height: 34px;
  line-height: 34px;
  top: 7px;
  right: 7px;
  position: absolute;
  display: inline-block;
}
.layout .search {
  width: 100%;
  background-color: #fff;
}
.layout .search > .tab {
  height: 50px;
  padding: 10px;
  box-sizing: border-box;
  position: relative;
  margin: 1px 0;
}
.layout .search > .tab > input:first-of-type {
  width: 100%;
  height: 30px;
  box-sizing: border-box;
  padding-left: 30px;
  border-radius: 15px;
  background-color: #f1f1f1;
}
.layout .search > .tab > input:last-of-type {
  position: absolute;
  top: 10px;
  right: 10px;
  width: 63px;
  height: 30px;
  background-color: #ff841d;
  color: #FFFFFF;
  border-radius: 15px;
}
.layout #brandTitle .brandTitle > span,
.layout #brandTitle2 .brandTitle > span {
  width: 100%;
  height: 40px;
  line-height: 40px;
  padding-left: 20px;
  padding-left: 10px;
  font-size: 14px;
  color: #5a5a5a;
  font-family: "SimSun ";
  border-top: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
}
.layout #brandTitle > .brandT-List-top,
.layout #brandTitle2 > .brandT-List-top {
  width: 98%;
  font-size: 16px;
  margin: 5px auto;
  background-color: #eee;
  height: 38px;
  line-height: 38px;
  color: #874604;
  font-family: "microsoft SimHei  ";
  padding-left: 10px;
  border: 1px solid #ccc;
}
.layout #brandTitle > ul > li,
.layout #brandTitle2 > ul > li {
  border-bottom: 1px solid #e8e8e8;
  border-top: 1px solid #ccc;
  margin: 0;
}
.layout #brandTitle > ul > li > a,
.layout #brandTitle2 > ul > li > a {
  width: 100%;
  position: relative;
  display: inline-block;
  height: 40px;
  line-height: 40px;
  font-weight: 900;
  color: #000;
  font-family: "microsoft SimHei ";
  margin: 0;
  font-size: 16px;
  padding-left: 20px;
}
.layout #brandTitle > ul > li > a > .icon,
.layout #brandTitle2 > ul > li > a > .icon {
  position: absolute;
  top: 6px;
  right: 15px;
  width: 30px;
  height: 30px;
  background-size: 10%;
  background: url("../../images/icon_bar.png") no-repeat -200px -29px;
  transform: rotate(-90deg) scale(0.5, 0.5);
}
.layout #brandTitle2 > ul > li {
  height: 50px;
  border-top: none;
}
.layout #brandTitle2 > ul > li > em {
  display: inline-block;
  width: 20px;
  height: 20px;
  line-height: 20px;
  color: #fff;
  background-color: #c9c9c9;
  text-align: center;
  vertical-align: top;
  font-size: 14px;
  margin: 10px;
}
.layout #brandTitle2 > ul > li > div {
  letter-spacing: 1px;
  display: inline-block;
  padding-top: 8px;
  color: #000;
  margin: 0;
  text-align: -webkit-match-parent;
  font-size: 16px;
  font-family: "NSimSun ";
  font-style: normal;
}
.layout #brandTitle2 > ul > li > div > a {
  color: #000;
}
.layout #brandTitle2 > ul > li > div > #count {
  font-size: 12px;
  color: #999;
}
.layout #brandTitle2 > ul > li:nth-of-type(1) > em {
  background-color: #f10e0e;
}
.layout #brandTitle2 > ul > li:nth-of-type(2) > em {
  background-color: #ff9314;
}
.layout #brandTitle2 > ul > li:nth-of-type(3) > em {
  background-color: #8adf5b;
}
.layout #brandTitle2 > ul > li:last-of-type {
  border-bottom: none;
}
.layout .selList {
  width: 100%;
}
.layout .selList > .selList-Top {
  height: 40px;
  line-height: 40px;
  font-size: 14px;
  color: #874604;
  font-family: "SimHei ";
  font-weight: 700;
  padding-left: 10px;
  border-bottom: 1px solid #ccc;
  border-top: 1px solid #ccc;
}
.layout .selList > .container {
  width: 100%;
  margin-left: -15px;
  margin-right: -15px;
}
.layout .selList > .container ul > li {
  width: 100%;
  height: 130px;
  border-bottom: 1px solid #e7e7e7;
}
.layout .selList > .container ul > li > .selList-List-l {
  padding-top: 9px;
}
.layout .selList > .container ul > li .selList-List-r p:first-of-type {
  font-size: 16px;
  color: #333;
  padding-top: 8px;
}
.layout .selList > .container ul > li .selList-List-r p:last-of-type {
  font-size: 18px;
  color: #ea3900;
  position: relative;
}
.layout .selList > .container ul > li .selList-List-r p:last-of-type > .stars {
  display: inline-block;
  position: absolute;
  right: 5px;
}
.layout .selList > .container ul > li .selList-List-r p:last-of-type > .stars .orange {
  color: #fc7f09;
  font-weight: 300;
}
.layout .selList > .container ul > li .selList-List-r p:last-of-type > .stars .gray {
  color: gray;
}
.layout .selList > .container ul > li .selList-List-r .comPeice {
  color: #999;
  font-size: 12px;
  padding-bottom: 20px;
}
.layout .selList > .container ul > li .selList-List-r .comPeice > span:first-child {
  padding-right: 20px;
  line-height: 24px;
}
.layout .selList > .container li:last-of-type {
  border-bottom: none;
}
.layout .selList-dis {
  width: 100%;
}
.layout .selList-dis > .selList-dis-Top {
  height: 40px;
  line-height: 40px;
  font-size: 14px;
  color: #874604;
  font-family: "SimHei ";
  font-weight: 700;
  padding-left: 10px;
  border-bottom: 1px solid #ccc;
  border-top: 1px solid #ccc;
}
.layout .selList-dis > .container {
  width: 100%;
}
.layout .selList-dis > .container .selList-dis-r > p {
  text-align: left;
  padding-top: 20px;
  font-size: 16px;
}
.layout .selList-dis > .container ul > li .selList-dis-user {
  width: 100%;
  font-size: 14px;
  font-family: "Microsoft simhei";
  color: #0782C1;
}
.layout .selList-dis > .container ul > li > .content {
  text-indent: 2em;
  color: #000;
  font-size: 14px;
  background-color: #f7f7f7;
  font-family: "Microsoft simhei  ";
  letter-spacing: 1px;
  line-height: 24px;
  margin-left: 10px;
}
.layout .loading {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 999;
  background-color: rgba(68, 68, 68, 0.5);
  display: none;
}
.layout .loading .loading-img {
  position: absolute;
  left: 50%;
  top: 50%;
  z-index: 1000;
  height: 50px;
  width: 50px;
  transform: translate(-50%, -50%);
}
