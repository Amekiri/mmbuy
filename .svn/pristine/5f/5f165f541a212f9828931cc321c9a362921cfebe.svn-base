  background-size: auto 100%;
  vertical-align: middle;
}
.layout > .main section.moreMS {
  width: 100%;
  padding: 10px;
  box-sizing: border-box;
}
.layout > .main section.moreMS > a {
  display: block;
  width: 100%;
  height: 30px;
  line-height: 30px;
  background-color: #F4F4F4;
  border: 1px solid #dddddd;
  border-radius: 5px;
  text-align: center;
  font-size: 12px;
  background-image: linear-gradient(#ffffff, #f4f4f4);
}
.layout footer {
  margin-top: 10px;
}
.layout footer > .top {
  width: 100%;
  box-sizing: border-box;
  height: 35px;
  font-size: 18px;
  padding-left: 10px;
  background-color: #ff9000;
}
.layout footer > .top > span {
  display: inline-block;
  float: left;
  line-height: 35px;
  color: #FFFFFF;
  font-weight: bold;
}
.layout footer > .top > img {
  width: 20px;
  height: 20px;
  float: right;
  margin-right: 10px;
  margin-top: 8px;
}
.layout footer > .function {
  height: 34px;
  margin-top: 10px;
}
.layout footer > .function > a {
  line-height: 34px;
  text-align: center;
  float: left;
  box-sizing: border-box;
  border: 1px solid #c4c4c4;
  border-left: 0 none;
  font-size: 12px;
}
.layout footer > .function > a:nth-of-type(1) {
  width: 30%;
}
.layout footer > .function > a:nth-of-type(2) {
  width: 30%;
}
.layout footer > .function > a:nth-of-type(3) {
  width: 40%;
  border-right: none;
}
.layout footer > .interview {
  margin-top: 20px;
}
.layout footer > .interview > p {
  text-align: center;
  font-size: 12px;
  color: #666;
}
.layout footer > .interview > p > span {
  color: red;
}
