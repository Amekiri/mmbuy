.layout {
  width: 100%;
}
.layout > header {
  width: 100%;
  height: 50px;
  background-color: #ff841d;
}
.layout > header > a {
  position: fixed;
  right: 20px;
  bottom: 138px;
  z-index: 999;
}
.layout > header > a > img {
  width: 60px;
}
.layout > header > h1 {
  width: 70%;
  height: 100%;
  padding: 12px;
  box-sizing: border-box;
  display: block;
}
.layout > header > h1 > a {
  display: block;
  height: 100%;
  width: 100%;
  margin-top: -4px;
}
.layout > header > h1 > a > img {
  height: 100%;
}
.layout > header > .download {
  height: 48px;
  width: 48px;
  padding: 7px;
}
.layout > header > .download > a {
  display: block;
  width: 100%;
  height: 100%;
}
.layout > header > .download > a > img {
  width: 100%;
}
.layout > .search {
  height: 50px;
  padding: 10px;
  box-sizing: border-box;
  position: relative;
  background-color: #fff;
  margin: 1px 0;
}
.layout > .search > input:first-of-type {
  width: 100%;
  height: 30px;
  box-sizing: border-box;
  padding-left: 30px;
  border-radius: 15px;
  background-color: #f1f1f1;
}
.layout > .search > input:last-of-type {
  position: absolute;
  top: 10px;
  right: 10px;
  width: 63px;
  height: 30px;
  background-color: #ff841d;
  color: #FFFFFF;
  border-radius: 15px;
}
.layout > .main {
  width: 100%;
}
.layout > .main > nav {
  width: 100%;
}
.layout > .main > nav > .nav-li-list {
  width: 100%;
  background-color: #fff;
}
.layout > .main > nav > .nav-li-list > li {
  width: 25%;
  float: left;
  margin: 16px 0px;
}
.layout > .main > nav > .nav-li-list > li > a {
  display: block;
  width: 100%;
  text-align: center;
}
.layout > .main > nav > .nav-li-list > li > a > img {
  width: 50px;
}
.layout > .main section.discount {
  width: 100%;
}
.layout > .main section.discount > .top {
  width: 100%;
  box-sizing: border-box;
  height: 35px;
  font-size: 18px;
  padding-left: 10px;
  background-color: #ff9000;
}
.layout > .main section.discount > .top > span {
  display: inline-block;
  float: left;
  line-height: 35px;
  color: #FFFFFF;
  font-weight: bold;
}
.layout > .main section.discount > .top > img {
  width: 20px;
  height: 20px;
  float: right;
  margin-right: 10px;
  margin-top: 8px;
}
.layout > .main section.discount > .dcProduct {
  width: 100%;
}
.layout > .main section.discount > .dcProduct > ul {
  width: 100%;
  background-color: #fff;
}
.layout > .main section.discount > .dcProduct > ul > li {
  border-bottom: 1px solid #e0e0e0 ;
  width: 100%;
  padding: 14px 8px;
}
.layout > .main section.discount > .dcProduct > ul > li > a {
  display: block;
  width: 100%;
}
.layout > .main section.discount > .dcProduct > ul > li > a img {
  width: 100px;
  height: 100px;
}
.layout > .main section.discount > .dcProduct > ul > li > a > .right {
  overflow: hidden;
  padding-left: 10px;
  height: 100px;
  position: relative;
}
.layout > .main section.discount > .dcProduct > ul > li > a > .right > p {
  font-family: '����';
}
.layout > .main section.discount > .dcProduct > ul > li > a > .right > p > .price {
  color: #ff6c00;
}
.layout > .main section.discount > .dcProduct > ul > li > a > .right > .productfrom,
.layout > .main section.discount > .dcProduct > ul > li > a > .right .producttime,
.layout > .main section.discount > .dcProduct > ul > li > a > .right .productComCount {
  margin-top: 45px;
  font-size: 12px;
  color: #a1a1a1 ;
}
.layout > .main section.discount > .dcProduct > ul > li > a > .right > .productComCount {
  float: right;
  margin-right: 15px;
}
.layout > .main section.discount > .dcProduct > ul > li > a > .right > .commentSection {
  float: right;
  margin-right: 20px;
}
