nav > .searchBox {
  display: none;
  background-color: #f7f7f7;
  position: absolute;
  border: 1px solid #e0e0e0;
  width: 100%;
  z-index: 99;
  top: 39px;
}
nav > .searchBox .searchBox-t {
  padding: 10px;
  width: 100%;
  position: relative;
  height: 42px;
  font-size: 15px;
}
nav > .searchBox .searchBox-t .txt {
  border: 1px solid #eee;
  height: 34px;
  margin-right: 50px;
  background-color: #fff;
}
nav > .searchBox .searchBox-t .txt > input {
  width: 100%;
  line-height: 34px;
  text-indent: 26px;
}
nav > .searchBox .searchBox-t > a {
  display: block;
  width: 30px;
  right: 20px;
  position: absolute;
  text-align: center;
  line-height: 34px;
  float: right;
}
nav > .searchBox .searchBox-b {
  font-size: 15px;
  padding: 10px;
}
nav > .searchBox .searchBox-b > span {
  display: block;
  padding: 8px 0;
}
nav > .searchBox .searchBox-b > .chooseList:first-of-type {
  border-bottom: 1px solid #e0e0e0;
}
nav > .searchBox .searchBox-b > .chooseList {
  width: 100%;
}
nav > .searchBox .searchBox-b > .chooseList dd {
  box-sizing: border-box;
  float: left;
  width: 25%;
  height: 32px;
  padding-right: 10px;
  line-height: 32px;
  margin-bottom: 10px;
  text-align: center;
  font-size: 13px;
}
nav > .searchBox .searchBox-b > .chooseList dd > a {
  border: 1px solid #eaeaea;
  display: block;
  border-radius: 3px;
}
nav > .searchBox .searchBox-b > .chooseList dd > .choose {
  background-color: #ff9501;
  color: #fff;
}
nav > .selectBox div {
  position: absolute;
  top: 38px;
  width: 100%;
  background-color: #fff;
  display: none;
  z-index: 99;
}
nav > .selectBox div li {
  height: 40px;
  border-bottom: 1px solid #dadada;
  line-height: 40px;
  position: relative;
}
nav > .selectBox div li > a {
  display: block;
  padding-left: 10px;
}
nav > .selectBox div li.current:after {
  content: '';
  display: block;
  top: 10px;
  background-image: url("data:image/svg+xml;charset=utf-8,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 1024 1024' width='20' height='20'><path fill='#ff4400' d='M959.94666 564.447904 899.86426 624.530304 370.356514 95.046095 124.134717 341.268916 64.052317 281.187539 361.641015-16.402183 370.051569-24.529281 379.068943-16.402183Z' transform='translate(0, 812) scale(1, -1)'/></svg>");
  position: absolute;
  right: 10px;
  width: 20px;
  height: 20px;
}
/*��Ʒ�б�*/
.products-list {
  padding: 5px;
}
.products-list li {
  box-sizing: border-box;
  width: 50%;
  padding: 5px;
  float: left;
}
.products-list li > a {
  background-color: #fff;
  display: block;
  height: 100%;
  width: 100%;
}
.products-list li > a .pic {
  width: 100%;
  height: auto;
  overflow: hidden;
  text-align: center;
}
.products-list li > a .pic > img {
  height: auto;
  max-width: 100%;
}
.products-list li > a > .pro_dis {
  color: #333;
  font-size: 12px;
  height: 14px;
  word-break: break-all;
  overflow: hidden;
  padding: 0 6px;
  line-height: 14px;
  margin-top: 6px;
}
.products-list li > a > .pro_price {
  font-size: 20px;
  display: block;
  padding: 6px;
  color: #f8601d;
}
.loading {
  margin: 30px 0;
  text-align: center;
  display: none;
}
.show {
  display: block;
}
