body,
html {
  height: 100%;
  width: 100%;
}
.coudan_layout {
  width: 100%;
  height: 100%;
}
.fixed {
  position: fixed;
  top: 0;
  z-index: 20;
  width: 100%;
}
header {
  height: 44px;
  background-color: #ff8b00;
  position: relative;
}
header > h1 {
  line-height: 44px;
  color: #fff;
  font-size: 20px;
  text-align: center;
  font-weight: normal;
}
header .goBack {
  display: block;
  color: #fff;
  font-weight: bold;
  height: 100%;
  position: absolute;
  left: 20px;
}
header .goBack .icon {
  display: inline-block;
  width: 1.5em;
  height: 1.5em;
  fill: currentColor;
  overflow: hidden;
  position: absolute;
  top: 50%;
  margin-top: -0.75em;
}
.active {
  display: block;
}
/*����*/
nav {
  height: 39px;
  background-color: #fff;
  border-top: 1px solid #dadada;
  border-bottom: 1px solid #dadada;
  position: relative;
}
nav > .filter {
  height: 100%;
  overflow: hidden;
}
nav > .filter ul li {
  float: left;
  width: 33.3%;
  text-align: center;
  line-height: 39px;
}
nav > .filter ul li > a {
  display: block;
  position: relative;
}
nav > .filter ul li > a > i {
  background-repeat: no-repeat;
  background-image: url("data:image/svg+xml;charset=utf-8,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 1024 1024' width='17' height='17'><path fill='#565656' d='M778.88 445.386667 528.106667 194.72 277.44 445.386667Z' transform='translate(0, 812) scale(1, -1)'/></svg>");
  width: 24px;
  height: 24px;
  display: inline-block;
  vertical-align: middle;
  background-size: 22px 22px ;
  background-position: center center;
  transition: .5s;
  transform-origin: center center;
}
nav > .filter ul li > a .arrow {
  transform: rotate(180deg);
}
nav > .search {
  width: 46px;
  float: right;
  height: 100%;
  border-left: 1px solid #dadada;
  text-align: center;
  position: relative;
}
