* {
  box-sizing: border-box;
}
body {
  background: #fff;
}
.top {
  background: #f4483e;
  height: 44px;
  line-height: 44px;
  color: #fff;
  text-align: center;
  position: relative;
}
.top > a {
  color: #fff;
  position: absolute;
  top: 0;
  font-size: 30px;
  width: 50px;
}
.top > a:nth-child(1) {
  left: 0;
}
.top > a:nth-child(1)::before {
  content: "";
  position: absolute;
  top: 11px;
  left: 10px;
  border: 10px solid #ccc;
  border-color: transparent #fff transparent transparent;
}
.top > a:nth-child(1)::after {
  content: "";
  position: absolute;
  top: 11px;
  left: 14px;
  border: 10px solid #ccc;
  border-color: transparent #f4483e transparent transparent;
}
.top > h1 {
  font-size: 20px;
  font-weight: bold;
}
.top > a:nth-last-child(1) {
  right: 0;
}
.top > a:nth-last-child(1) > img {
  margin-top: 6px;
  width: 32px;
}
.center {
  background: #fff;
  margin: 10px 0;
}
.center > ul {
  width: 100%;
}
.center > ul > li {
  float: left;
  width: 33.3%;
}
.center > ul > li > .li {
  text-align: center;
  font-size: 12px;
  margin-left: 5px;
  margin-top: 5px;
  height: 82px;
  padding-top: 5px;
  border: 1px solid #eeeeee;
}
.center > ul > li > .li > a {
  height: 75px;
  display: block;
}
.center > ul > li > .li > a > img {
  width: 50px;
}
.nav {
  border-top: 1px solid #ebebeb;
  border-bottom: 1px solid #ccc;
  background: linear-gradient(to bottom, #fff, #ebebeb);
  line-height: 36px;
  padding-left: 8px;
  margin-top: 20px;
  margin-bottom: 25px;
}
.nav > a {
  display: inline-block;
  line-height: 16px;
  font-size: 14px;
  color: #000;
}
.footer {
  padding: 0;
}
.footer > .footerT {
  border-top: 1px solid #c4c4c4;
  border-bottom: 1px solid #c4c4c4;
  height: 34px;
  width: 100%;
}
.footer > .footerT > a {
  float: left;
  height: 34px;
  line-height: 34px;
  text-align: center;
  width: 30%;
  color: #000;
  font-size: 14px;
}
.footer > .footerT > a > img {
  width: 16px;
  margin-top: 4px;
}
.footer > .footerT > a:nth-of-type(2) {
  border-left: 1px solid #c4c4c4;
  border-right: 1px solid #c4c4c4;
}
.footer > .footerT > a:nth-child(3) {
  width: 40%;
}
.footer > .footerB {
  padding-top: 15px;
}
.footer > .footerB > p {
  text-align: center;
  height: 16px;
  line-height: 16px;
  font-size: 12px;
}
.footer > .footerB > p > a {
  display: inline-block;
  line-height: 16px;
}
.footer > .footerB > p > a:nth-child(1) {
  color: #f4483e;
}
.footer > .footerB > p > a:nth-child(2) {
  color: #000;
}
.ad {
  text-align: center;
  font-size: 12px;
  line-height: 16px;
  margin: 8px 0;
  color: #999;
}
#rate-shop1 > li {
  border-bottom: 1px solid #e7e7e7;
}
#rate-shop1 > li > a {
  display: inline-block;
  width: 100%;
  padding: 10px;
}
#rate-shop1 > li > a .shop1-left {
  float: left;
  margin-right: 10px;
}
#rate-shop1 > li > a .shop1-left > img {
  width: 100px;
  height: 80px;
}
#rate-shop1 > li > a .shop1-right {
  position: relative;
  line-height: 24px;
}
#rate-shop1 > li > a .shop1-right > .shop1-right-L > h2 {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  height: 40px;
  font-size: 14px;
  line-height: 20px;
  font-weight: normal;
}
#rate-shop1 > li > a .shop1-right > .shop1-right-L > p:nth-of-type(1) {
  line-height: 14px;
  height: 20px;
}
#rate-shop1 > li > a .shop1-right > .shop1-right-L > p:nth-of-type(2) {
  font-size: 12px;
  color: #EE7700;
  margin-top: 10px;
}
#rate-shop1 > li > a .shop1-right > .shop1-right-R {
  position: absolute;
  top: 0;
  right: 0;
  width: 30px;
  height: 100%;
  background: url("data:image/svg+xml;charset=utf-8,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 1024 1024'><path fill='#999999' d='M746.255255 299.653099 746.33098 299.728824 713.12674 332.933063 713.049992 332.857339 297.859875 748.048479 264.635169 714.82275 679.816077 299.642866 264.635169-115.538041 297.859875-148.762747 713.00906 266.386438 713.085808 266.30969 746.341213 299.567141Z' transform='translate(0, 812) scale(1, -1)'/></svg>") no-repeat;
  background-size: 20px 20px ;
  background-position: center center;
}
.bBoth {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.8);
  z-index: 99;
  position: fixed;
  top: 0;
  left: 0;
  text-align: center;
  display: none;
}
.bBoth .both {
  height: 100%;
}
.bBoth .both > img {
  position: absolute;
  transform: translate(-50%, -50%);
  left: 50%;
  top: 50%;
  width: 300px;
}
.bBoth > i {
  font-size: 80px;
  color: #fff;
  position: absolute;
  transform: translateY(-50%);
  top: 50%;
}
.bBoth > i:nth-of-type(1) {
  left: 70px;
}
.bBoth > i:nth-of-type(2) {
  right: 70px;
}
